@{
    ViewData["Title"] = "Home Page";
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}
 

<div class="container mt-5 mb-5">

    <div class="col-lg-12  ">
        <div class="section-title text-center   ">
            <h2 class="section-main-title">@loc.localizer["ControlPanel"]  </h2>
        </div>
    </div>
    <div class="menu-container">



        <div class="menu-card">
            <div class="menu-header">@loc.localizer["Ideas"]</div>
            <ul class="submenu">
               
                <li onclick="openNewApp('/admin/iconsetting')">@loc.localizer["Icons"]</li>
                

                <li onclick="openNewApp('/admin/videosetting')">  @loc.localizer["BackgroundVideo"] </li>

            </ul>
        </div>
        <div class="menu-card">
            <div class="menu-header">@loc.localizer["Announcement"]</div>
            <ul class="submenu">
                <li onclick="openNewApp('/admin/iconsetting')">@loc.localizer["Announcement"]</li>                 

            </ul>
        </div>
    </div>
</div>

